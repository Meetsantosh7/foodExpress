<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2196F3;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#64B5F6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="url(#bg)"/>
  
  <!-- Sushi Plate -->
  <ellipse cx="150" cy="130" rx="70" ry="20" fill="#f5f5f5" stroke="#ddd" stroke-width="2"/>
  
  <!-- Sushi Roll 1 -->
  <circle cx="120" cy="110" r="15" fill="#fff"/>
  <circle cx="120" cy="110" r="12" fill="#ff6b6b"/>
  <circle cx="120" cy="110" r="8" fill="#90ee90"/>
  <circle cx="120" cy="110" r="4" fill="#ffd700"/>
  
  <!-- Sushi Roll 2 -->
  <circle cx="150" cy="110" r="15" fill="#fff"/>
  <circle cx="150" cy="110" r="12" fill="#ff9800"/>
  <circle cx="150" cy="110" r="8" fill="#4caf50"/>
  <circle cx="150" cy="110" r="4" fill="#f44336"/>
  
  <!-- Sushi Roll 3 -->
  <circle cx="180" cy="110" r="15" fill="#fff"/>
  <circle cx="180" cy="110" r="12" fill="#9c27b0"/>
  <circle cx="180" cy="110" r="8" fill="#ffeb3b"/>
  <circle cx="180" cy="110" r="4" fill="#795548"/>
  
  <!-- Nigiri -->
  <ellipse cx="110" cy="130" rx="12" ry="8" fill="#fff"/>
  <ellipse cx="110" cy="125" rx="12" ry="4" fill="#ff6347"/>
  
  <ellipse cx="190" cy="130" rx="12" ry="8" fill="#fff"/>
  <ellipse cx="190" cy="125" rx="12" ry="4" fill="#ff9800"/>
  
  <!-- Chopsticks -->
  <rect x="50" y="70" width="2" height="80" fill="#8B4513" transform="rotate(15 51 110)"/>
  <rect x="55" y="70" width="2" height="80" fill="#8B4513" transform="rotate(15 56 110)"/>
  
  <!-- Wasabi -->
  <path d="M200 120 Q205 115 210 120 Q205 125 200 120" fill="#90ee90"/>
  
  <!-- Restaurant Name -->
  <rect x="20" y="160" width="260" height="30" fill="rgba(0,0,0,0.7)" rx="5"/>
  <text x="150" y="180" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Sushi Zen</text>
</svg>
