<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4CAF50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8BC34A;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="url(#bg)"/>
  
  <!-- Burger Base (bottom bun) -->
  <ellipse cx="150" cy="130" rx="50" ry="15" fill="#deb887"/>
  
  <!-- Lettuce -->
  <ellipse cx="150" cy="125" rx="45" ry="8" fill="#90ee90"/>
  
  <!-- Patty -->
  <ellipse cx="150" cy="115" rx="40" ry="12" fill="#8B4513"/>
  
  <!-- Cheese -->
  <ellipse cx="150" cy="105" rx="42" ry="6" fill="#ffd700"/>
  
  <!-- Tomato -->
  <ellipse cx="150" cy="100" rx="38" ry="5" fill="#ff6347"/>
  
  <!-- Top Bun -->
  <ellipse cx="150" cy="90" rx="45" ry="18" fill="#deb887"/>
  
  <!-- Sesame Seeds -->
  <circle cx="130" cy="85" r="2" fill="#fff"/>
  <circle cx="145" cy="82" r="2" fill="#fff"/>
  <circle cx="160" cy="85" r="2" fill="#fff"/>
  <circle cx="170" cy="90" r="2" fill="#fff"/>
  
  <!-- French Fries -->
  <rect x="200" y="90" width="4" height="25" fill="#ffd700" rx="2"/>
  <rect x="206" y="85" width="4" height="30" fill="#ffd700" rx="2"/>
  <rect x="212" y="88" width="4" height="27" fill="#ffd700" rx="2"/>
  <rect x="218" y="92" width="4" height="23" fill="#ffd700" rx="2"/>
  
  <!-- Restaurant Name -->
  <rect x="20" y="160" width="260" height="30" fill="rgba(0,0,0,0.7)" rx="5"/>
  <text x="150" y="180" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Burger Barn</text>
</svg>
