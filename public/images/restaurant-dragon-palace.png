<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B0000;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF6347;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="300" height="200" fill="url(#bg)"/>
  
  <!-- Dragon -->
  <ellipse cx="150" cy="100" rx="60" ry="30" fill="#ffd700" stroke="#daa520" stroke-width="2"/>
  
  <!-- Dragon Head -->
  <ellipse cx="120" cy="85" rx="25" ry="20" fill="#ffd700" stroke="#daa520" stroke-width="2"/>
  
  <!-- Dragon Eyes -->
  <circle cx="110" cy="80" r="3" fill="#ff0000"/>
  <circle cx="125" cy="80" r="3" fill="#ff0000"/>
  
  <!-- Dragon Whiskers -->
  <path d="M95 85 Q90 82 85 85" stroke="#000" stroke-width="1" fill="none"/>
  <path d="M95 90 Q90 93 85 90" stroke="#000" stroke-width="1" fill="none"/>
  
  <!-- Rice Box -->
  <rect x="170" y="80" width="40" height="30" fill="#fff" stroke="#ddd" stroke-width="2" rx="3"/>
  <ellipse cx="190" cy="85" rx="15" ry="3" fill="#f5f5dc"/>
  
  <!-- Chopsticks -->
  <rect x="220" y="70" width="2" height="40" fill="#8B4513" transform="rotate(15 221 90)"/>
  <rect x="225" y="70" width="2" height="40" fill="#8B4513" transform="rotate(15 226 90)"/>
  
  <!-- Fortune Cookie -->
  <path d="M250 120 Q260 110 270 120 Q260 130 250 120" fill="#deb887" stroke="#cd853f" stroke-width="2"/>
  
  <!-- Restaurant Name -->
  <rect x="20" y="160" width="260" height="30" fill="rgba(0,0,0,0.7)" rx="5"/>
  <text x="150" y="180" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Dragon Palace</text>
</svg>
